{ 
  "FileLogging": { 
    "DefaultPath": "log", //默认文件地址
    "DefaultMaxMB": 500, //默认最大文件大小
    "DefaultFileName": "yyyyMMdd", //默认文件名字
    "DefaultRollingType": "Day", //默认文件新建方式
    //过滤器，以.分割，逐层匹配
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "NetCoreWebApp": "Debug",
      "NewFile": "Debug"
    }
  }
}
